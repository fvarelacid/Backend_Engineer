$smcT5.CustomScripts.SelectBroadband = function(){
    var broadBandElem = document.querySelector('.basket-container .basket-broadband .monthly-items .basket__item-label');
    if(broadBandElem){
        if(broadBandElem.textContent === "Brilliant Broadband"){
            $smcT5.CookieManager.create('smc_brilliant_broad', true);
            $smcT5.CookieManager.create('smc_fibre_broad', false);
            $smcT5.CookieManager.create('smc_super_fibre_broad', false);
        }else if(broadBandElem.textContent === "Fab Fibre"){
            $smcT5.CookieManager.create('smc_brilliant_broad', false);
            $smcT5.CookieManager.create('smc_fibre_broad', true);
            $smcT5.CookieManager.create('smc_super_fibre_broad', false);
        }else if(broadBandElem.textContent === "Super Fibre"){
            $smcT5.CookieManager.create('smc_brilliant_broad', false);
            $smcT5.CookieManager.create('smc_fibre_broad', false);
            $smcT5.CookieManager.create('smc_super_fibre_broad', true);
        }
    }
};

$smcT5.CustomScripts.EntertaimentCtaClicked = function(){
    var cookie = $smcT5.CookieManager.read('smc_entertainment_clicked');
    if(cookie){
        return false;
    }else{
        return true;
    }
};

$smcT5.CustomScripts.CinemaCtaClicked = function(){
    var cookie = $smcT5.CookieManager.read('smc_cinema_clicked');
    if(cookie){
        return false;
    }else{
        return true;
    }
};

$smcT5.CustomScripts.SportsCtaClicked = function(){
    var cookie = $smcT5.CookieManager.read('smc_sports_clicked');
    if(cookie){
        return false;
    }else{
        return true;
    }
